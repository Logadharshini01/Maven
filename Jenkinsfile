pipeline {
    agent any

    stages {
        stage('Build now') {
            steps {
                echo 'Build app first- will build?'
            }
        }
	stage('Test now') {
            steps {
                echo 'test app second- will test?'
            }
        }
	stage('Deploy now') {
            steps {
                echo 'deploy app third- will deploy'
            }
        }
    }
}
